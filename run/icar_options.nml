terrain_smooth_cycles=5, 
decay_rate_L_topo=1.6667, 
decay_rate_S_topo=13.0, 
sleve_n=1.35
/

&var_list
uvar="u", 
vvar="v", 
pvar="pressure", 
tvar="theta", 
qvvar="qv", 
hgtvar="height", 
zvar="z", 
latvar="lat_m", 
lonvar="lon_m", 
lat_hi="lat_hi", 
lon_hi="lon_hi", 
hgt_hi="hgt_hi", 
time_var="time"
/

&parameters
forcing_start_date="2020-12-01 00:00:00", 
end_date="2020-12-01 02:00:00", 
calendar="standard", 
inputinterval=3600, 
dx=1000, 
qv_is_relative_humidity=false, 
ideal=True, 
debug=True, 
readdz=true, 
nz=50, 
z_is_geopotential=False, 
z_is_on_interface=False, 
t_is_potential=True, 
time_varying_z=False, 
use_agl_height=True, 
agl_cap=400, 
