&model_version
version=2.1, 
comment="flat_z_height=-10"
/

&output_list
names="pressure","temperature","lon","lat","z","dz","dz_i","u","v","w","w_grid","qv","terrain", 
outputinterval=1200, 
output_file="icar_out_", 
restartinterval=3600, 
restart_file="icar_rst_"
/

&physics
pbl=0, 
lsm=0, 
water=0, 
mp=0, 
rad=0, 
conv=0, 
adv=1, 
wind=3
/

&adv_parameters
flux_corr=1,
h_order=5,
v_order=3
/

&time_parameters
RK3=.True.,
cfl_strictness=4,
cfl_reduction_factor=1.4
/

&files_list
init_conditions_file="init.nc", 
boundary_files="forcing.nc", 
forcing_file_list=""
/

&z_info
dz_levels=500, 500, 500, 500, 500, 500, 500, 500, 500, 500, 500, 500, 500, 500, 500, 500, 500, 500, 500, 500, 500, 500, 500, 500, 500, 500, 500, 500, 500, 500, 500, 500, 500, 500, 500, 500, 500, 500, 500, 500, 500, 500, 500, 500, 500, 500, 500, 500, 500, 500, 
space_varying=.True., 
flat_z_height=-10, 
